  "scripts": {
    "dev": "npm --prefix backend run dev",
    "migrate": "npx sequelize-cli db:migrate --config backend/databases/postgres/config.json",
    "seed": "npx sequelize-cli db:seed:all --config backend/databases/postgres/config.json",
    "reset": "npx sequelize-cli db:migrate:undo:all --config backend/databases/postgres/config.json && npm run migrate && npm run seed",
